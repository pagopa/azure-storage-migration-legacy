{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "io-platform-migration-toolkit base jtsconfig",
  "compilerOptions": {
    "lib": ["ES2022"],
    "module": "CommonJS",
    "target": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noEmit": true
  }
}
